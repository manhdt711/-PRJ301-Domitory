<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Controller.AccountController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>/bookbed</servlet-name>
        <servlet-class>Controller.bookbed</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>confirmbook</servlet-name>
        <servlet-class>Controller.CfBook</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>res</servlet-name>
        <servlet-class>Controller.res</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>His</servlet-name>
        <servlet-class>Controller.His</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>main</servlet-name>
        <servlet-class>Controller.main</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insPOW</servlet-name>
        <servlet-class>Controller.insertPow</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PowWater</servlet-name>
        <servlet-class>Controller.Powwater</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>report</servlet-name>
        <servlet-class>Controller.report</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insReport</servlet-name>
        <servlet-class>Controller.insReport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>/bookbed</servlet-name>
        <url-pattern>/bookbed</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>confirmbook</servlet-name>
        <url-pattern>/confirmbook</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>res</servlet-name>
        <url-pattern>/res</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>His</servlet-name>
        <url-pattern>/his</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>main</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insPOW</servlet-name>
        <url-pattern>/insPOW</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PowWater</servlet-name>
        <url-pattern>/PowWater</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>report</servlet-name>
        <url-pattern>/report</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insReport</servlet-name>
        <url-pattern>/insReport</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
